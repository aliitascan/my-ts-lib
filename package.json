{
  "name": "thekhaos-my-ts-lib",
  "private": false,
  "version": "0.0.1",
  "description": "This is a simple utility package",
  "author": "aliitascan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aliitascan/my-ts-lib.git"
  },
  "bugs": {
    "url": "https://github.com/aliitascan/my-ts-lib/issues"
  },
  "keywords": ["khaos", "test", "package", "first"],
